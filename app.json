{
  "expo": {
    "name": "iluvus-react-native",
    "slug": "iluvus-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "scheme": "iluvus",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-image-picker",
      "react-native-compressor"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We needs camera permissions to allow you to upload profile photos.",
        "NSPhotoLibraryAddUsageDescription": "We needs photo library permissions to allow you to upload profile images.",
        "NSPhotoLibraryUsageDescription": "We needs photo library permissions to allow you to upload profile images."
      },
      "bundleIdentifier": "com.anonymous.iluvus-react-native"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "CAMERA_ROLL"
      ],
      "package": "com.anonymous.iluvusreactnative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a2c46b59-3ebc-4799-9324-3b748c8a23fe"
      }
    },
    "owner": "anhthuan250901",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a2c46b59-3ebc-4799-9324-3b748c8a23fe"
    }
    
  }
}
